{
  "name": "preact-app",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rm -rf ./build/*",
    "dev": "npm run clean && node --inspect=0.0.0.0:9229 ./node_modules/.bin/webpack-dev-server",
    "lint": "prettier-standard 'src/**/*.{js,ts}' && standard --fix --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin 'src/!(definitions)/**/*.{js,ts}'",
    "prod": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "start": "node ./build/server",
    "test": "tsc --noEmit && standard --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin 'src/!(definitions)/**/*.{js,ts}' && jest",
    "preversion": "npm run test && git add -A",
    "postversion": "git push && git push --tags",
    "watch": "npm run build -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/runtime": "^7.7.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.4.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preact": "^10.0.4",
    "prettier-standard": "^15.0.1",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "standard": "^14.3.1",
    "styled-components": "^4.4.1",
    "supertest": "^4.0.2",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "whatwg-fetch": "^3.0.0",
    "yaml-loader": "^0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^~/(.*)": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  }
}
